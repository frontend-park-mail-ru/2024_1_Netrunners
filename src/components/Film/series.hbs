<ul class="series-block">
    <div class="series-block__seasons">
        some buttons for seasons
    </div>
    {{#each series}}
        <li class="series-block__element">
            <span href="{{this.link}}"> {{ this.title }} </span>
            <div class="film-content-block__data-element">
                {{ this.duration }}
            </div>
        </li>
    {{/each}}
</ul>
